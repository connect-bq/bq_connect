{
  "version": 2,
  "builds": [
    {
      "src": "Frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/login",
      "destination": "./public/login.html"
    },
    {
      "source": "/register",
      "destination": "/public/register.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/dashboard.html"
    },
    {
      "source": "/about",
      "destination": "/public/about.html"
    }
  ]
}